@model IEnumerable<WebApplication2.Models.STOCK_RECEIVED_NOTE>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StockReceivedNoteDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PUBLISHER.PublisherName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StockReceivedNoteDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PUBLISHER.PublisherName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.StockReceivedNoteID }) |
                @Html.ActionLink("Details", "Details", new { id = item.StockReceivedNoteID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.StockReceivedNoteID }) |
            </td>
        </tr>
    }

</table>
